<?xml version="1.0"?>
<package xmlns:metal="http://xml.zope.org/namespaces/metal"
         xmlns:tal="http://xml.zope.org/namespaces/tal">
  <metadata tal:define="semVer options/version;
                        extraVer options/versionExtra;
                        versionSuffix csharp:!String.IsNullOrEmpty(extraVer)? String.Concat(&quot;-&quot;, extraVer) : String.Empty;
                        fullVersion string:$semVer$versionSuffix;">
    <id>CSF.Zpt.Cli</id>
    <version tal:content="fullVersion">0.0.0</version>
    <title>CSF.Zpt.Cli (ZPT-Sharp component)</title>
    <authors>CSF Software Ltd</authors>
    <licenseUrl>https://opensource.org/licenses/MIT</licenseUrl>
    <projectUrl>http://csf-dev.github.io/ZPT-Sharp/</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Command-line page renderer for ZPT-Sharp.  This is a minimal package with no plugins; consider the ZPT-Sharp-ZptBuilder package.  For more information see the project website.</description>
    <copyright tal:content="string:Copyright ${options/copyrightYear}">Copyright 2016</copyright>
    <dependencies>
      <dependency id="CSF.Zpt"
                  version="[0.0.0,1.0.0)"
                  tal:attributes="version string:[$fullVersion,${options/nextBreakingVersion})" />
      <dependency id="CSF.Cli.Parameters" version="[1.0.3,2.0.0)" />
      <dependency id="CSF.Utils" version="[6.1.1,7.0.0)" />
      <dependency id="CSF.Reflection" version="[1.0.3,2.0.0)" />
    </dependencies> 
  </metadata>
  <files>
    <file src="bin/Release/ZptBuilder.exe" target="lib/net45" />
    <file src="bin/Release/ZptBuilder.exe.config" target="lib/net45" />
    <file src="bin/Release/Doc/ZptBuilder.MANUAL.txt" target="content/net45" />
  </files>
</package>
