<?xml version="1.0"?>
<package xmlns:metal="http://xml.zope.org/namespaces/metal"
         xmlns:tal="http://xml.zope.org/namespaces/tal">
  <metadata>
    <id>CSF.Zpt.DocumentProviders.HtmlHAP</id>
    <version tal:define="semVer options/version;
                         extraVer options/versionExtra;
                         versionSuffix csharp:String.IsNullOrEmpty(extraVer)? String.Concat(&quot;-&quot;, extraVer) : String.Empty;
                         fullVersion string:$semVer$versionSuffix;"
             tal:content="fullVersion">0.0.0</version>
    <title>ZPT-Sharp HTML document provider plugin</title>
    <authors>CSF Software Ltd</authors>
    <licenseUrl>https://opensource.org/licenses/MIT</licenseUrl>
    <projectUrl>http://csf-dev.github.io/ZPT-Sharp/</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>This is a plugin for ZPT-Sharp which provides support for rendering HTML documents, using the HTML Agility Pack.</description>
    <copyright tal:content="string:Copyright ${options/copyrightYear}">Copyright 2016</copyright>
    <dependencies>
      <dependency id="CSF.Zpt"
                  version="[0.0.0,1.0.0)"
                  tal:attributes="version string:[${options/version},${options/nextBreakingVersion})" />
      <dependency id="HtmlAgilityPack" version="1.4.9" />
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Xml.dll" />
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="bin/Release/CSF.Zpt.DocumentProviders.HtmlHAP.dll" target="lib/net45" />
    <file src="bin/Release/CSF.Zpt.DocumentProviders.HtmlHAP.xml" target="lib/net45" />
    <file src="app.config.install.xdt" target="content" />
    <file src="app.config.uninstall.xdt" target="content" />
  </files>
</package>
