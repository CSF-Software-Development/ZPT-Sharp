<?xml version="1.0"?>
<package xmlns:metal="http://xml.zope.org/namespaces/metal"
         xmlns:tal="http://xml.zope.org/namespaces/tal">
  <metadata tal:define="semVer options/version;
                        extraVer options/versionExtra;
                        versionSuffix csharp:!String.IsNullOrEmpty(extraVer)? String.Concat(&quot;-&quot;, extraVer) : String.Empty;
                        fullVersion string:$semVer$versionSuffix;">
    <id>CSF.Zpt.MVC5</id>
    <version tal:content="fullVersion">0.0.0</version>
    <title>CSF.Zpt.MVC5 (ZPT-Sharp component)</title>
    <authors>CSF Software Ltd</authors>
    <licenseUrl>https://opensource.org/licenses/MIT</licenseUrl>
    <projectUrl>http://csf-dev.github.io/ZPT-Sharp/</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>ZPT-Sharp ASP.NET MVC5 ViewEngine.  This is a minimal package with no plugins; consider the ZPT-Sharp-MVC5 package.  For more information see the project website.</description>
    <copyright tal:content="string:Copyright ${options/copyrightYear}">Copyright 2016</copyright>
    <dependencies>
      <dependency id="CSF.Zpt"
                  version="[0.0.0,1.0.0)"
                  tal:attributes="version string:[$fullVersion,${options/nextBreakingVersion})" />
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="Microsoft.Web.Infrastructure.dll" />
      <frameworkAssembly assemblyName="System.Web.Helpers.dll" />
      <frameworkAssembly assemblyName="System.Web.Mvc.dll" />
      <frameworkAssembly assemblyName="System.Web.Razor.dll" />
      <frameworkAssembly assemblyName="System.Web.WebPages.Deployment.dll" />
      <frameworkAssembly assemblyName="System.Web.WebPages.dll" />
      <frameworkAssembly assemblyName="System.Web.WebPages.Razor.dll" />
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="bin/Release/CSF.Zpt.MVC5.dll" target="lib/net45" />
  </files>
</package>
