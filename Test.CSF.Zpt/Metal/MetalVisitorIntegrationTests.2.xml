<?xml version="1.0" encoding="UTF-8" ?>
<root xmlns="http://ns.csf-dev.com/sample" xmlns:metal="http://xml.zope.org/namespaces/metal">
  <blah metal:define-macro="macro-one" metal:extend-macro="macro-base">
    <bar>Extender content</bar>
    <blurb metal:fill-slot="slot-one">
      Extra slot decoration
      <spong metal:define-slot="slot-one">Throwaway content</spong>
    </blurb>
    <bar>Extender content</bar>
    <bar>Extender content</bar>
  </blah>
  <ele metal:define-macro="macro-two">
    <baz>BAZ content</baz>
    <spong metal:define-slot="slot-one">Throwaway content</spong>
    <baz>BAZ content</baz>
    <spong metal:define-slot="slot-two">Throwaway content</spong>
    <baz>BAZ content</baz>
  </ele>
  <ele metal:define-macro="macro-base">
    <base>Base content</base>
    <spong metal:define-slot="slot-one">Throwaway content</spong>
    <base>Base content</base>
    <spong metal:define-slot="slot-two">Throwaway content</spong>
    <base>Base content</base>
  </ele>
</root>
