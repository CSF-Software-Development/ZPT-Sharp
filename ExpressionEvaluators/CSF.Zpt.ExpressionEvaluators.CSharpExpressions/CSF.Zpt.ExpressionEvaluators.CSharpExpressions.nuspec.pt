<?xml version="1.0"?>
<package xmlns:metal="http://xml.zope.org/namespaces/metal"
         xmlns:tal="http://xml.zope.org/namespaces/tal">
  <metadata tal:define="semVer options/version;
                        extraVer options/versionExtra;
                        versionSuffix csharp:!String.IsNullOrEmpty(extraVer)? String.Concat(&quot;-&quot;, extraVer) : String.Empty;
                        fullVersion string:$semVer$versionSuffix;">
    <id>CSF.Zpt.ExpressionEvaluators.CSharpExpressions</id>
    <version tal:content="fullVersion">0.0.0</version>
    <title>ZPT-Sharp 'csharp' expression evaluator plugin</title>
    <authors>CSF Software Ltd</authors>
    <licenseUrl>https://opensource.org/licenses/MIT</licenseUrl>
    <projectUrl>http://csf-dev.github.io/ZPT-Sharp/</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>This is a plugin for ZPT-Sharp which provides support for evaluating CSharp expressions.</description>
    <copyright tal:content="string:Copyright ${options/copyrightYear}">Copyright 2016</copyright>
    <dependencies>
      <dependency id="CSF.Zpt"
                  version="[0.0.0,1.0.0)"
                  tal:attributes="version string:[$fullVersion,${options/nextBreakingVersion})" />
      <dependency id="CSF.Utils" version="[6.1.1,7.0.0)" />
      <dependency id="CSF.Configuration" version="[1.1.2,2.0.0)" />
      <dependency id="CSF.Caches" version="[1.0.2,2.0.0)" />
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="Microsoft.CSharp" />
      <frameworkAssembly assemblyName="System.Core" />
      <frameworkAssembly assemblyName="System.Dynamic" />
    </frameworkAssemblies>
  </metadata>
  <files>
    <file src="bin/Release/CSF.Zpt.ExpressionEvaluators.CSharpFramework.dll" target="lib/net45" />
    <file src="bin/Release/CSF.Zpt.ExpressionEvaluators.CSharpExpressions.dll" target="lib/net45" />
    <file src="bin/Release/CSF.Zpt.ExpressionEvaluators.CSharpFramework.xml" target="lib/net45" />
    <file src="bin/Release/CSF.Zpt.ExpressionEvaluators.CSharpExpressions.xml" target="lib/net45" />
    <file src="app.config.install.xdt" target="content" />
    <file src="app.config.uninstall.xdt" target="content" />
    <file src="web.config.install.xdt" target="content" />
    <file src="web.config.uninstall.xdt" target="content" />
  </files>
</package>
