<?xml version="1.0"?>
<package xmlns:metal="http://xml.zope.org/namespaces/metal"
         xmlns:tal="http://xml.zope.org/namespaces/tal">
  <metadata>
    <id>CSF.Zpt.ExpressionEvaluators.PathExpressions</id>
    <version tal:content="options/version">0.0.0</version>
    <title>ZPT-Sharp 'path' expression evaluator plugin</title>
    <authors>CSF Software Ltd</authors>
    <licenseUrl>https://opensource.org/licenses/MIT</licenseUrl>
    <projectUrl>http://csf-dev.github.io/ZPT-Sharp/</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>This is a plugin for ZPT-Sharp which provides support for evaluating path and local path expressions.</description>
    <copyright tal:content="string:Copyright ${options/copyrightYear}">Copyright 2016</copyright>
    <dependencies>
      <dependency id="CSF.Zpt"
                  version="[0.0.0,1.0.0)"
                  tal:attributes="version string:[${options/version},${options/nextBreakingVersion})" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin/Release/CSF.Zpt.ExpressionEvaluators.PathExpressions.dll" target="lib/net45" />
    <file src="bin/Release/CSF.Zpt.ExpressionEvaluators.PathExpressions.xml" target="lib/net45" />
    <file src="app.config.install.xdt" target="content" />
    <file src="app.config.uninstall.xdt" target="content" />
  </files>
</package>
