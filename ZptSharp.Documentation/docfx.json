{
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [
            "ZptSharp/ZptSharp.csproj",
            "ZptSharp.Impl/ZptSharp.Impl.csproj",
            "ZptSharp.Cli/ZptSharp.Cli.csproj",
            "ZptSharp.Hosting/ZptSharp.Hosting.csproj",
            "DocumentProviders/**/*.csproj",
            "Evaluators/**/*.csproj",
            "MvcViewEngines/ZptSharp.Mvc5/ZptSharp.Mvc5.csproj",
            "MvcViewEngines/ZptSharp.MvcCore/ZptSharp.MvcCore.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": ["**/README.md"]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}